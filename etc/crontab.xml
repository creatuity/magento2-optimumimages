<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="creatuity_optimumimages_cron_optimizer" instance="Creatuity\OptimumImages\Cron\Optimizer" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="creatuity_optimumimages_cron_cleaner" instance="Creatuity\OptimumImages\Cron\Cleaner" method="execute">
            <schedule>0 0 0 * *</schedule>
        </job>
    </group>
</config>
